{
  "name": "umami",
  "productName": "umami",
  "version": "2.0.0-alpha",
  "private": true,
  "author": "Trilitech <umami-admin@trili.tech>",
  "description": "Tezos Wallet",
  "homepage": "./",
  "engines": {
    "node": ">=18.15.0 <19.0.0"
  },
  "main": "public/electron.js",
  "scripts": {
    "build-storybook": "storybook build",
    "build": "yarn generate-ts-types && react-app-rewired build",
    "eject": "react-scripts eject",
    "electron:package:linux": "electron-builder -l -c.extraMetadata.main=build/electron.js",
    "electron:package:mac:debug": "cross-env CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder -m -c.extraMetadata.main=build/electron.js",
    "electron:package:mac": "electron-builder -m -c.extraMetadata.main=build/electron.js",
    "electron:package:win": "electron-builder -w -c.extraMetadata.main=build/electron.js",
    "electron:start": "electronmon .",
    "format:ci": "prettier --check .",
    "format": "prettier --write .",
    "generate-ts-types": "graphql-codegen --config graphql.config.yml && prettier --write src/graphql/generated.ts",
    "lint:ci": "eslint src --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "start": "GENERATE_SOURCEMAP=false && react-app-rewired start",
    "storybook": "storybook dev -p 6006",
    "test:tezos:integration": "cross-env TZ=CET react-scripts test --verbose --watchPathIgnorePatterns=[]",
    "test": "cross-env TZ=CET react-scripts test --resetModules --coverage --verbose --testPathIgnorePatterns='src/integration/*'",
    "theme:watch": "chakra-cli tokens src/style/theme.ts --watch",
    "theme": "chakra-cli tokens src/style/theme.ts",
    "typecheck": "tsc"
  },
  "browserslist": {
    "production": [
      "last 1 electron version"
    ],
    "development": [
      "last 1 electron version"
    ]
  },
  "devDependencies": {
    "@airgap/beacon-wallet": "^4.0.6",
    "@babel/runtime": "^7.22.10",
    "@chakra-ui/cli": "^2.4.1",
    "@chakra-ui/icons": "^2.1.0",
    "@chakra-ui/react": "^2.8.0",
    "@chriscdn/promise-semaphore": "^2.0.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@graphql-codegen/cli": "3.3.1",
    "@graphql-codegen/typed-document-node": "4.0.1",
    "@graphql-codegen/typescript": "3.0.4",
    "@graphql-codegen/typescript-operations": "3.0.4",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@ledgerhq/hw-transport-webhid": "^6.27.18",
    "@reduxjs/toolkit": "^1.9.5",
    "@storybook/addon-essentials": "^7.3.0",
    "@storybook/addon-interactions": "^7.3.0",
    "@storybook/addon-links": "^7.3.0",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.3.0",
    "@storybook/preset-create-react-app": "^7.3.0",
    "@storybook/react": "^7.3.0",
    "@storybook/react-webpack5": "^7.3.0",
    "@storybook/testing-library": "^0.2.0",
    "@taquito/ledger-signer": "^17.2.0",
    "@taquito/michel-codec": "^17.2.0",
    "@taquito/rpc": "^17.2.0",
    "@taquito/signer": "^17.2.0",
    "@taquito/taquito": "^17.2.0",
    "@taquito/utils": "^17.2.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "13.5.0",
    "@toruslabs/customauth": "^15.0.5",
    "@types/assert": "^1.5.6",
    "@types/jest": "27.5.1",
    "@types/node": "18.15.0",
    "@types/papaparse": "^5.3.7",
    "@types/pluralize": "^0.0.30",
    "@types/promise-retry": "^1.1.3",
    "@types/prop-types": "^15.7.5",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "@types/redux-mock-store": "^1.0.3",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/testing-library__user-event": "^4.2.0",
    "@types/wait-on": "^5.3.1",
    "@tzkt/oazapfts": "^4.5.2",
    "@tzkt/sdk-api": "^2.2.0",
    "assert": "^2.0.0",
    "axios": "^1.4.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "bip39": "^3.1.0",
    "buffer": "^6.0.3",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
    "electron": "^23.3.12",
    "electron-builder": "^23.6.0",
    "electron-notarize": "^1.2.2",
    "electronmon": "^2.0.2",
    "eslint": "^8.47.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-unused-imports": "^3.0.0",
    "framer-motion": "^10.15.2",
    "graphql": "^16.8.0",
    "graphql-request": "^6.1.0",
    "https-browserify": "^1.0.0",
    "jest-fail-on-console": "^3.1.1",
    "lodash": "^4.17.21",
    "mockdate": "^3.0.5",
    "nano-id": "^1.1.0",
    "os-browserify": "^0.3.0",
    "papaparse": "^5.4.1",
    "pluralize": "^8.0.0",
    "prettier": "^2.8.8",
    "process": "^0.11.10",
    "promise-retry": "^2.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-app-rewired": "^2.2.1",
    "react-datocms": "^4.1.3",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-hook-form": "^7.45.4",
    "react-hooks-testing-library": "^0.6.0",
    "react-icons": "^4.10.1",
    "react-identicons": "^1.2.5",
    "react-native-safe-area-context": "3.4.1",
    "react-native-svg": "12.5.1",
    "react-native-web": "0.17.7",
    "react-qrcode-logo": "^2.9.0",
    "react-query": "^3.39.3",
    "react-redux": "8.0.5",
    "react-remove-scroll": "^2.5.6",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^6.15.0",
    "react-scripts": "5.0.1",
    "react-slick": "^0.29.0",
    "redux": "^4.2.1",
    "redux-mock-store": "^1.5.4",
    "redux-persist": "^6.0.0",
    "source-map-loader": "^4.0.1",
    "storybook": "^7.3.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "typescript": "5.0.4",
    "wait-on": "^7.0.1",
    "web-vitals": "^2.1.4",
    "webpack": "^5.88.2",
    "webpack-remove-code-blocks": "^0.1.5",
    "zod": "^3.22.0"
  },
  "jest": {
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs",
      "@ledgerhq/devices/hid-framing": "@ledgerhq/devices/lib/hid-framing",
      "@tzkt/oazapfts/runtime": "@tzkt/oazapfts/lib/runtime"
    },
    "watchPathIgnorePatterns": [
      "src/integration"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 76,
        "statements": 76
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules",
      "src/mocks/*",
      "src/utils/useAssetsPolling.ts",
      "src/*integration*",
      "src/stories/*",
      "src/.*\\.stories\\.tsx?"
    ]
  },
  "build": {
    "appId": "com.trilitech.umami",
    "productName": "Umami",
    "directories": {
      "buildResources": "public"
    },
    "mac": {
      "icon": "./public/logo512.png",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "target": [
        {
          "target": "dmg",
          "arch": [
            "arm64",
            "x64"
          ]
        }
      ]
    },
    "win": {
      "icon": "build/logo512.png",
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "linux": {
      "icon": "build/logo512.png",
      "target": [
        "deb",
        "rpm"
      ],
      "category": "Finance",
      "mimeTypes": [
        "x-scheme-handler/umami"
      ]
    },
    "snap": {
      "plugs": [
        "default",
        "raw-usb",
        "hidraw"
      ],
      "buildPackages": [
        "libusb-1.0-0-dev",
        "libudev-dev"
      ],
      "stagePackages": [
        "libusb-1.0-0",
        "libnss3"
      ]
    },
    "protocols": {
      "name": "umami",
      "schemes": [
        "umami"
      ]
    },
    "afterSign": "scripts/notarize.js"
  },
  "packageManager": "yarn@3.5.0",
  "resolutions": {
    "nwsapi": "2.2.2"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
